    <select id="getRzPrjapplyAll" parameterType="rzPrjapplyQuery" resultMap="rzPrjapplyResult">
		SELECT 
		<include refid="rzPrjapplyField" />  
		from  
		rz_prjapply<include refid="condition" />
	</select>
	
    <!-- 查询所有 -->
    <select id="getRzPrjapplyBatchId" parameterType="list" resultMap="rzPrjapplyResult">
		SELECT 
		<include refid="rzPrjapplyField" />  
		from  
		rz_prjapply
		where 
		pk_prjapply in 
    	<foreach item="item" index="index" collection="list" open="(" separator="," close=")">
    		#{item} 
    	</foreach>
	</select>
	
	<!-- 分页查询总数 -->
	<select id="getRzPrjapplyByPageCount" parameterType="rzPrjapplyQuery" resultType="int">
		SELECT count(*) from  rz_prjapply
		<include refid="condition" />
	</select>
	
	<!-- 分页查询 -->
	<select id="getRzPrjapplyByPage" parameterType="rzPrjapplyQuery" resultMap="rzPrjapplyResult">
		SELECT <include refid="rzPrjapplyField" /> from rz_prjapply
		<include refid="condition" />
		<if test="sort !=null">
		    ORDER BY ${sort} ${order} 
		</if>
		<![CDATA[LIMIT #{startRow}, #{endRow} ]]>
	</select>
	
	<!-- 根据Id查询 -->
	<select id="getRzPrjapplyById" parameterType="rzPrjapplyQuery" resultMap="rzPrjapplyResult">
		SELECT <include refid="rzPrjapplyField" /> from  rz_prjapply  where pk_prjapply = #{pkPrjapply} 
		and ifnull(dr,0) = 0
		 
	</select>
    
</mapper>
