<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="pubref">

	<!-- 查询参照数据 -->
	<select id="getRefDataValueBySql" resultType="java.util.HashMap"
		parameterType="java.lang.String">
		${value}
	</select>
	<!-- 查询参照数据总记录数 -->
	<select id="getRefDataCountbySql" resultType="java.lang.Integer"
		parameterType="java.lang.String">
		${value}
	</select>

</mapper>
