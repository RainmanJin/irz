<!-- search start  -->
<div class="searchPanel padding-10">
	<form class="form-horizontal">
		<div class="searchContent" data-bind="with:queryData">
						
			<div class="row">
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">项目合同主键</label>
						<div class="col-md-8 col-sm-8">
							<input type="text" name="pkPrjState" class="form-control" data-bind="value:pkPrjState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">0 报价单生成、1 立项中、2 项目评审中、3合同签订中、4 项目已起租、5 项目租后管理中、6 项目结清、7 项目合同作废</label>
						<div class="col-md-8 col-sm-8">
							<input type="text" name="prjState" class="form-control" data-bind="value:prjState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">报价测算主键</label>
						<div class="col-md-8 col-sm-8">
							<input type="text" name="pkPricecal" class="form-control" data-bind="value:pkPricecal">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">项目评审主键</label>
						<div class="col-md-8 col-sm-8">
							<input type="text" name="pkPrjreview" class="form-control" data-bind="value:pkPrjreview">
						</div>
					</div>
				</div>
			</div>
			<!--******更多条件放这里********-->
			<div class="hideRule">
				
				<div class="row">
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">立项申请主键</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="pkPrjapply" class="form-control" data-bind="value:pkPrjapply">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">合同主键</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="pkPrjcontr" class="form-control" data-bind="value:pkPrjcontr">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">原合同主键</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="pkPrjcontrori" class="form-control" data-bind="value:pkPrjcontrori">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">合同编号</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="contrCode" class="form-control" data-bind="value:contrCode">
							</div>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">操作人员</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="pkUserOper" class="form-control" data-bind="value:pkUserOper">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">组织</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="pkOrg" class="form-control" data-bind="value:pkOrg">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">部门主键</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="pkDeptdoc" class="form-control" data-bind="value:pkDeptdoc">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">时间戳</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="ts" class="form-control" data-bind="value:ts">
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- 展开更多的一行-->
		<div class="searchToolsbar">
			<div data-target=".hideRule" data-toggle="collapse" class="moreRule">
				<span>展开更多查询条件</span>
				<i class="glyphicon glyphicon-triangle-bottom"></i>
			</div>
			<div class="search-box">
				<button type="button" class="btn btn-default btn-sm reset-btn">重置</button>
				<button type="button" class="btn btn-default btn-sm search-btn">查询</button>
			</div>
		</div>
	</form>
</div>
<!-- search end -->