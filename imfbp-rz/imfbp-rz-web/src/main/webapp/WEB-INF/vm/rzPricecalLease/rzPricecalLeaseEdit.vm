<div class="panel panel-default">
	<div class="panel-heading ">
		<button type="button" class="btn btn-default btn-sm save-btn">保存</button>
		<button type="button" class="btn btn-default btn-sm back-btn">取消</button>
	</div>
	<div class="panel-body" data-bind="with:editData">
		<form class="rzPricecalLeaseEditForm form-horizontal">
			<div class="row">
				<div class="col-md-3 col-sm-6" style="display:none">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">主键</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="pkPricecal" class="form-control" data-bind="value:pkPricecal,enable:enableState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">期数</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="num" class="form-control" data-bind="value:num,enable:enableState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">应还款日期</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="rptDate" class="form-control" data-bind="value:rptDate,enable:enableState">
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">偿还期间</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="paydur" class="form-control" data-bind="value:paydur,enable:enableState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">期间天数</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="durdays" class="form-control" data-bind="value:durdays,enable:enableState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">租赁利率(%)</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="leaseRate" class="form-control" data-bind="value:leaseRate,enable:enableState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">应还款本金</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="rptAmt" class="form-control" data-bind="value:rptAmt,enable:enableState">
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">应还款利息</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="rptInt" class="form-control" data-bind="value:rptInt,enable:enableState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">应还款租金</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="rptRent" class="form-control" data-bind="value:rptRent,enable:enableState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">应支付手续费</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="payFee" class="form-control" data-bind="value:payFee,enable:enableState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">应还款合计</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="rptTotal" class="form-control" data-bind="value:rptTotal,enable:enableState">
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">剩余本金</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="residualAmt" class="form-control" data-bind="value:residualAmt,enable:enableState">
						</div>
					</div>
				</div>
			</div>
		</form>
	</div>
</div>


