<div class="panel panel-default">
	<div class="panel-heading footer-saveOrCancel" >
		<button type="button" class="btn btn-default btn-sm save-btn">保存</button><button type="button" class="btn btn-default btn-sm back-btn">返回</button>
	</div>
		
	<div class="panel-body form-parent" data-bind="with:editData">
		<form class="rzPrjreviewGuarEditForm form-horizontal">
			<div class="rateBaseInfo row panel slideParent">
            
                <div class="col-xs-12 panel-heading" data-toggle="collapse" data-target="#rateBaseInfo">
                     <h5>项目担保方主信息</h5><span class="glyphicon glyphicon-chevron-up"></span>
                </div>
                <div class="col-xs-12 panel-body collapse in" id="rateBaseInfo">
			<div class="row">
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">担保人编码</label>
						<!-- <div class="col-md-9 col-sm-8">
							<input type="text" name="pkCustomer" class="form-control content-w210-input" data-bind="value:pkCustomer,enable:enableState">
						</div> -->
						<div class="col-md-9 col-sm-8">
							<!-- <input type="text" name="pkCustomer" class="form-control content-w210-input" data-bind="value:pkCustomer,enable:enableState"> -->
							 <input type="text" name="pkCustomer" class="form-control content-w210-input"
                                    data-bind="enable:false"
                                    data-imfbp-ref-bind="pkCustomer"
                                    data-imfbp-ref-bind-field="" data-imfbp-ref-value="">
                             <div class="icon-wrap ">
                                 <i class="glyphicon glyphicon-th-list icon" id="icon"
                                    name="pkCustomer"
                                    data-imfbp-ref-name='customerref'
                                    data-imfbp-ref-use = 'true'
                                    data-imfbp-ref-fun="customerRefCallFunction" data-imfbp-ref="ref"></i>
                             </div>
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">担保人类别</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="" class="form-control content-w210-input" data-bind="enable:false">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">证件类型</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="" class="form-control content-w210-input" data-bind="enable:false">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">证件号码</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="enterprisePersonIdentificationNo" class="form-control content-w210-input" data-bind="value:enterprisePersonIdentificationNo,enable:false">
						</div>
					</div>
				</div>

				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">法人代表</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="enterpriseLegalPerson" class="form-control content-w210-input" data-bind="value:enterpriseLegalPerson,enable:false">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">通信地址</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="enterpriseAddr" class="form-control content-w210-input" data-bind="value:enterpriseAddr,enable:false">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">邮编</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="enterprisePostcode" class="form-control content-w210-input" data-bind="value:enterprisePostcode,enable:false">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">担保方式</label>
						<div class="col-md-9 col-sm-8">
							<!-- <input type="text" name="guarType" class="form-control content-w210-input" data-bind="value:guarType,enable:enableState"> -->
							<select name="guarType" class="form-control"
								data-bind="value:guarType,enable:enableState">
								<option value="0">信用</option>
								<option value="1">质押</option>
								<option value="2">抵押</option>
								<option value="3">保证</option>
								<option value="4">综合担保</option>
								<option value="5">其他</option>
							</select>
						</div>
					</div>
				</div>

				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">担保金额</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="guarAmt" class="form-control content-w210-input" data-bind="value:guarAmt,enable:enableState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">抵押金额</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="mortgageAmt" class="form-control content-w210-input" data-bind="value:mortgageAmt,enable:enableState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">质押金额</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="pledgeAmt" class="form-control content-w210-input" data-bind="value:pledgeAmt,enable:enableState">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">提供担保原因</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="guarReson" class="form-control content-w210-input" data-bind="value:guarReson,enable:enableState">
						</div>
					</div>
				</div>

				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">联系人</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="enterpriseContacts" class="form-control content-w210-input" data-bind="value:enterpriseContacts,enable:false">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">联系人电话</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="enterpriseTelphone" class="form-control content-w210-input" data-bind="value:enterpriseTelphone,enable:false">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">传真</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="enterpriseFax" class="form-control content-w210-input" data-bind="value:enterpriseFax,enable:false">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">联系人手机</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="enterpriseMobilephone" class="form-control content-w210-input" data-bind="value:enterpriseMobilephone,enable:false">
						</div>
					</div>
				</div>
	
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">联系人邮箱</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="enterpriseMail" class="form-control content-w210-input" data-bind="value:enterpriseMail,enable:false">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-3 col-sm-4 control-label no-padding-right">备注</label>
						<div class="col-md-9 col-sm-8">
							<input type="text" name="remark" class="form-control content-w210-input" data-bind="value:remark,enable:enableState">
						</div>
					</div>
				</div>
			</div>
		</form>
	</div>
	<div class="toolsbarPanelB" id="listButton">
		<div class="btn-group" role="group" id="btn_B" style="margin-top:40px>
		<button type="button"></button>
			<button type="button" class="btn btn-default btn-sm addguarb-btn">新增</button>
			<button type="button" class="btn btn-default btn-sm delete-btn">删除</button>
			<button type="button" class="btn btn-default btn-sm update-btn">修改</button>
		</div>
	</div>
	<h5 style="height: 56px;background: #EFF3F6;line-height: 56px;padding-left: 15px;border-radius: 4px 4px 0 0;margin-bottom:0;">项目担保方子信息</h5>
	<div class="grid rzPrjreviewGuarBEditList"></div>
	<ul id="pagination" class="pagination" style="display:none"></ul>
</div>
