<!-- search start  -->
<div class="searchPanel padding-10">
	<form class="form-horizontal">
		<div class="searchContent col-xs-8" data-bind="with:queryDataPlan">
						
			<div class="row">
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">主键</label>
						<div class="col-md-8 col-sm-8">
							<input type="text" name="pkOverdueDeratePlan" class="form-control" data-bind="value:pkOverdueDeratePlan">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">主表主键</label>
						<div class="col-md-8 col-sm-8">
							<input type="text" name="pkOverdueDerate" class="form-control" data-bind="value:pkOverdueDerate">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">期数</label>
						<div class="col-md-8 col-sm-8">
							<input type="text" name="num" class="form-control" data-bind="value:num">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">序号</label>
						<div class="col-md-8 col-sm-8">
							<input type="text" name="seqno" class="form-control" data-bind="value:seqno">
						</div>
					</div>
				</div>
			</div>
			<!--******更多条件放这里********-->
			<div class="hideRule">
				
				<div class="row">
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">应收款日期</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="receivableDate" class="form-control" data-bind="value:receivableDate">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">应收款合计(元)</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="receivableTotal" class="form-control" data-bind="value:receivableTotal">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">实收款合计(元)</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="realTotal" class="form-control" data-bind="value:realTotal">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">应收未收合计(元)</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="receivableNrcyTotal" class="form-control" data-bind="value:receivableNrcyTotal">
							</div>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">逾期天数</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="overdueDays" class="form-control" data-bind="value:overdueDays">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">逾期利率(%)</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="overdueRate" class="form-control" data-bind="value:overdueRate">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">应收罚息(元)</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="receivableDefInt" class="form-control" data-bind="value:receivableDefInt">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">应收未收罚息(元)</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="receivableNrcyDefInt" class="form-control" data-bind="value:receivableNrcyDefInt">
							</div>
						</div>
					</div>
				</div>
				<div class="row">
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">实际减免罚息(元)</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="actOverdur" class="form-control" data-bind="value:actOverdur">
							</div>
						</div>
					</div>
					<div class="col-md-3 col-sm-6">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right">本次申请减免罚息(元)</label>
							<div class="col-md-8 col-sm-8">
								<input type="text" name="curOverdurApply" class="form-control" data-bind="value:curOverdurApply">
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- 展开更多的一行-->
		<div class="searchToolsbar">
			<div data-target=".hideRule" data-toggle="collapse" class="moreRule">
				<span>展开更多查询条件</span>
				<i class="glyphicon glyphicon-triangle-bottom"></i>
			</div>
			<div class="search-box">
				<button type="button" class="btn btn-default btn-sm reset-btn">重置</button>
				<button type="button" class="btn btn-default btn-sm search-btn">查询</button>
			</div>
		</div>
	</form>
</div>
<!-- search end -->