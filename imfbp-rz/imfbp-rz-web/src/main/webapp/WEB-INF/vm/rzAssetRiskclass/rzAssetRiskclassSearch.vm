<!-- search start  -->
<div class="searchPanel padding-10">
	<form class="form-horizontal">
		<div class="searchContent" data-bind="with:queryData">

			<div class="row">
				<div class="col-md-3 col-sm-6 label-and-input-outdiv" style="width:216px;">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">客户名称</label>
						<div class="col-md-8 col-sm-8" style="width:160px;">
							<input type="text" name="pkCustomer" class="form-control header-w150-input"
	                                data-bind="enable:false"
	                                data-imfbp-ref-bind="pkCustomer"
	                                data-imfbp-ref-bind-field="userName" data-imfbp-ref-value="">
	                         <div class="icon-wrap ">
	                             <i class="glyphicon glyphicon-th-list icon" id="icon"
	                                data-imfbp-ref-name='customerref' name="pkCustomer"
	                                data-imfbp-ref-use = 'true'
	                                data-imfbp-ref-fun="headerRefCallFunction" data-imfbp-ref="ref"></i>
	                         </div>
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6 label-and-input-outdiv" style="width:216px;">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">项目名称</label>
						<div class="col-md-8 col-sm-8" style="width:160px;">
							<!-- <input type="text" name="pkPrjapply" class="form-control"
								data-bind="value:pkPrjapply"> -->
							<input type="text" name="pkPrjapply" class="form-control header-w150-input"
                                    data-bind="enable:false"
                                    data-imfbp-ref-bind="pkPrjapply"
                                    data-imfbp-ref-bind-field="" data-imfbp-ref-value="">
                             <div class="icon-wrap ">
                                 <i class="glyphicon glyphicon-th-list icon" id="icon"
                                    name="pkPrjapply"
                                    data-imfbp-ref-name='rzprjapplyConditionref'
                                    data-imfbp-ref-use = 'true'
                                    data-imfbp-ref-fun="prjreviewRefCallFunction" data-imfbp-ref="ref"></i>
                             </div>
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6 label-and-input-outdiv" style="width:216px;">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right" style="line-height:17px">风险分类变更日期</label>
						<div class="col-md-8 col-sm-8" style="width:160px;">
							<div class="input-group date form_hhg_date col-md-5"
								data-date="" data-date-format="yyyy-MM-dd"
								data-link-field="visitDate" data-link-format="yyyy-mm-dd">
								<input class="form-control header-w150-input" size="16" name="changeDate"
									type="text" data-bind="value:changeDate,enable:enableState"
									value="" readonly style="width: 111px !important;"> <span
									class="input-group-addon"><span
									class="glyphicon glyphicon-calendar"></span></span>
							</div>
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6 label-and-input-outdiv" style="width:216px;">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">合同名称</label>
						<div class="col-md-8 col-sm-8" style="width:160px;">
							<input type="text" name="pkPrjcontr" class="form-control header-w150-input"
                                   data-bind="enable:false"
                                   data-imfbp-ref-bind="pkPrjcontr"
                                   data-imfbp-ref-bind-field="contrName" data-imfbp-ref-value="">
                            <div class="icon-wrap ">
                                <i class="glyphicon glyphicon-th-list icon" id="icon"
                                   name="pkPrjcontr"
                                   data-imfbp-ref-name='rzprjcontrref'
                                   data-imfbp-ref-use = 'true'
                                   data-imfbp-ref-fun="headerRefCallFunction" data-imfbp-ref="ref"></i>
                            </div>
						</div>
					</div>
				</div>
			</div>
			<!--******更多条件放这里********-->
			<div class="hideRule">
				<div class="row">
					<div class="col-md-3 col-sm-6 label-and-input-outdiv" style="width:216px;">
						<div class="form-group">
							<label class="col-md-4 col-sm-4 control-label no-padding-right" style="line-height:17px">合同起租日期</label>
							<div class="col-md-8 col-sm-8" style="width:160px;">
								<!-- <input type="text" name="startLeaseDate" class="form-control"
									data-bind="value:startLeaseDate"> -->
								<div class="input-group date form_hhg_date col-md-5"
									data-date="" data-date-format="yyyy-MM-dd"
									data-link-field="visitDate" data-link-format="yyyy-mm-dd">
									<input class="form-control header-w150-input" size="16" name="startLeaseDate"
										type="text" data-bind="value:startLeaseDate,enable:enableState"
										value="" readonly style="width: 111px !important;"> <span
										class="input-group-addon"><span
										class="glyphicon glyphicon-calendar"></span></span>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- 展开更多的一行-->
		<div class="searchToolsbar">
			<div data-target=".hideRule" data-toggle="collapse" class="moreRule">
				<span>展开更多查询条件</span> <i class="glyphicon glyphicon-triangle-bottom"></i>
			</div>
			<div class="search-box">
				<button type="reset" class="btn btn-default btn-sm reset-btn">重置</button>
				<button type="button" class="btn btn-default btn-sm search-btn">查询</button>
			</div>
		</div>
	</form>
</div>
<!-- search end -->