<!-- search start  -->
<div class="searchPanel padding-10">
	<form class="form-horizontal">
		<div class="searchContent" data-bind="with:queryData">
			<div class="row">
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">模板编码</label>
						<div class="col-md-8 col-sm-8">
							<input type="text" name="templateCode" class="form-control" data-bind="value:templateCode">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">模板名称</label>
						<div class="col-md-8 col-sm-8">
							<input type="text" name="templateName" class="form-control" data-bind="value:templateName">
						</div>
					</div>
				</div>
				<div class="col-md-3 col-sm-6">
					<div class="form-group">
						<label class="col-md-4 col-sm-4 control-label no-padding-right">模板状态</label>
						<div class="col-md-8 col-sm-8">
							<!-- <input type="text" name="templateStatus" class="form-control" data-bind="value:templateStatus"> -->
							<select name="templateStatus" class="form-control  header-w150-input"
                                    data-bind="value:templateStatus">
                                <option value="">全部</option>
                                <option value = "0">已停用</option>
                                <option value = "1">已启用</option>
                                <option value = "2">已分配</option>
                            </select>
						</div>
					</div>
				</div>
				<div class="searchToolsbar">
					<div class="search-box">
						<button type="button" class="btn btn-default btn-sm reset-btn">重置</button>
						<button type="button" class="btn btn-default btn-sm search-btn">查询</button>
					</div>
				</div>
			</div>
		</div>
		
	</form>
</div>
<!-- search end -->